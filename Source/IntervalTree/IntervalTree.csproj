<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>10</LangVersion>
        <RootNamespace>Jamarino.IntervalTree</RootNamespace>
        <AssemblyName>Jamarino.IntervalTree</AssemblyName>
    </PropertyGroup>

    <PropertyGroup>
        <PackageId>Jamarino.IntervalTree</PackageId>
        <Version>1.0.0-alpha</Version>
        <Authors>Jonathan Marino</Authors>
        <Description>Interval tree implementation focused on memory efficiency. Similar to the RangeTree implementation, but uses as little as &lt;10% of the memory in comparison.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/jamarino/IntervalTree</PackageProjectUrl>
        <PackageTags>light interval tree intervaltree rangetree</PackageTags>
    </PropertyGroup>

</Project>
